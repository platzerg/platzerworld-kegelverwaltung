<?xml version="1.0" encoding="utf-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 <servlet>
  <servlet-name>CreateNew</servlet-name>
  <servlet-class>com.platzerworld.kegelverwaltung.servlet.ServletCreate</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>Remove</servlet-name>
  <servlet-class>com.platzerworld.kegelverwaltung.servlet.ServletRemove</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>LigaVerwaltungServlet</servlet-name>
  <servlet-class>com.platzerworld.kegelverwaltung.servlet.LigaVerwaltungServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>KlassenServlet</servlet-name>
  <servlet-class>com.platzerworld.kegelverwaltung.servlet.klasse.KlassenServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>MannschaftServlet</servlet-name>
  <servlet-class>com.platzerworld.kegelverwaltung.servlet.mannschaft.MannschaftServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>SpielerServlet</servlet-name>
  <servlet-class>com.platzerworld.kegelverwaltung.servlet.spieler.SpielerServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>VereinServlet</servlet-name>
  <servlet-class>com.platzerworld.kegelverwaltung.servlet.verein.VereinServlet</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>Remove</servlet-name>
  <url-pattern>/done</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>CreateNew</servlet-name>
  <url-pattern>/new</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>LigaVerwaltungServlet</servlet-name>
  <url-pattern>/Ligaverwaltung</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>KlassenServlet</servlet-name>
  <url-pattern>/Klassen</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>MannschaftServlet</servlet-name>
  <url-pattern>/Mannschaften</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>SpielerServlet</servlet-name>
  <url-pattern>/Spieler</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>VereinServlet</servlet-name>
  <url-pattern>/Verein</url-pattern>
 </servlet-mapping>
 <welcome-file-list>
  <welcome-file>Ligaverwaltung.jsp</welcome-file>
 </welcome-file-list>
</web-app>
